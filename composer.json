{
	"name": "w7/rangine",
	"description": "基于 Swoole 的常驻内存型 PHP 高性能框架",
	"type": "library",
	"license": "Apache-2.0",
	"authors": [
		{
			"name": "RenChao",
			"email": "donknap@gmail.com"
		}
	],
	"require": {
		"php": ">=7.2",
		"ext-json": "*",
		"w7/rangine-compat": "2.4.1",
		"w7/rangine-contract": "*",
		"w7/rangine-facade": "*",
		"w7/rangine-http-message": "~2.2.0",
		"w7/rangine-database-tool": "~1.1.0",
		"w7/rangine-lang": "~1.1.0",
		"w7/rangine-command": "~2.1.0",
		"w7/rangine-package-plugin": "*",
		"illuminate/config": "^6.0",
		"illuminate/database": "^6.0",
		"illuminate/events": "^6.0",
		"illuminate/validation": "^6.0",
		"illuminate/pagination": "^6.0",
		"nikic/fast-route": "^1.3",
		"monolog/monolog": "^1.2",
		"vlucas/phpdotenv": "^3.3",
		"symfony/console": "^4.3|^5.0",
		"filp/whoops": "^2.7",
		"twig/twig": "^3.0",
		"pimple/pimple": "^3.2"
	},
	"require-dev": {
		"swoole/ide-helper": "^4.4",
		"w7/php-cs-fixer": "*"
	},
	"autoload": {
		"psr-4": {
			"W7\\": "Src/"
		},
		"files": [
			"Src/Core/Helper/Functions.php",
			"Src/Core/Config/Define.php"
		]
	},
	"bin": [
		"bin/rangine-server",
		"bin/rangine-gerent"
	]
